<!-- reviewDetails.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>


</head>
<body>
    <h1>Review Details</h1>
    <p>Review ID: <%= reviewDetails.ReviewID %></p>
    <p>Restaurant Name: <%= reviewDetails.RestName %></p>
    <p>Street Address: <%= reviewDetails.StreetAddress %></p>
    <p>First Name: <%= reviewDetails.FirstName %></p>
    <p>Dessert Name: <%= reviewDetails.DessertName %></p>
    <p>Description: <%= reviewDetails.Description %></p>
    <p>Stars given: <%= reviewDetails.Stars %></p>
    <p>Price: $<%= reviewDetails.Price %></p>
    <p>Is it Dairy-Free? <%= reviewDetails.DairyFree === 'Y' ? 'Yes' : 'No' %></p>
    <p>Is it Gluten-Free? <%= reviewDetails.GlutenFree === 'Y' ? 'Yes' : 'No' %></p>

</body>
</html>

